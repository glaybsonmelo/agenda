<% if(errors.length > 0) { %> 
    <div class="col my-3">
        <div class="alert alert-danger">
            <% errors.forEach(error =>  {%>
                <%= error.msg %> <br>
            <%}) %>
        </div>
    </div>
<% } %> 

<% if(success.length > 0) { %> 
    <div class="col my-3">
        <div class="alert alert-success">
            <% success.forEach(error =>  {%>
                <%= success %> <br>
            <%}) %>
        </div>
    </div>
<% } %> 